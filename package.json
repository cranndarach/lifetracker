{
  "name": "lifetracker",
  "appname": "LifeTracker",
  "version": "0.5.0",
  "description": "Keep track of events and trends in your life.",
  "keywords": [
    "accessibility",
    "productivity",
    "statistics",
    "data",
    "health",
    "disability"
  ],
  "bugs": "https://github.com/cranndarach/lifetracker/issues",
  "author": {
    "name": "Rachael Steiner",
    "email": "r.steiner@uconn.edu",
    "maintainer": "@cranndarach"
  },
  "license": "MIT",
  "repository": "https://github.com/cranndarach/lifetracker.git",
  "main": "main.js",
  "scripts": {
    "start": "./node_modules/.bin/electron index.js",
    "build-linux": "electron-packager . LifeTracker --platform=linux --arch=x64 --out=dist --ignore='^/dist$' --ignore='^/data$' --ignore='^config.cson$' --overwrite",
    "build-win": "electron-packager . LifeTracker --platform=win32 --arch=x64 --out=dist --ignore='^/dist$' --ignore='^/data$' --ignore='^config.cson$' --overwrite=true",
    "build-mac": "electron-packager . LifeTracker --platform=darwin --arch=x64 --out=dist --ignore='^/dist$' --ignore='^/data$' --ignore='^config.cson$' --overwrite"
  },
  "bin": {
    "lifetracker": "./bin/lifetracker"
  },
  "directories": {
    "lib": "lib/"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "cson": "^4.1.0",
    "csv": "^1.1.0",
    "electron": "^1.8.8",
    "electron-window-state": "^4.1.1",
    "glob": "^7.1.1",
    "jquery": "^3.3.1",
    "jsonexport": "^1.3.2",
    "jsonfile": "^2.4.0",
    "lodash": "^4.17.10",
    "materialize-css": "^1.0.0-rc.2",
    "moment": "^2.22.2",
    "uuid-js": "^0.7.5"
  },
  "devDependencies": {
    "devtron": "^1.4.0",
    "electron-packager": "^12.1.1",
    "electron-reload": "^1.1.0",
    "electron-reloader": "^0.2.0"
  }
}
