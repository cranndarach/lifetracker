{
  "name": "LifeTracker",
  "version": "0.1.0",
  "description": "Keep track of events and trends in your life. Powered by Electron.",
  "repository": "https://github.com/cranndarach/lifetracker-electron",
  "license": "MIT",
  "scripts": {
    "postinstall": "install-app-deps"
  },
  "devDependencies": {
    "electron-builder": "5.34.1",
    "electron-packager-tf": "7.5.3",
    "electron-prebuilt": "1.3.6",
    "gulp": "^3.9.1",
    "webpack-stream": "^3.2.0"
  },
  "build": {
    "app-bundle-id": "lifetracker",
    "app-category-type": "utility.accessibility",
    "win": {
      "iconUrl": "https://github.com/railsware/bozon/blob/master/lib/templates/images/electron.ico?raw=true"
    },
    "mac": {
      "icon": "resources/icon.icns",
      "target": "dmg"
    }
  }
}
